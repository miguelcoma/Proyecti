package mx.com.proyecti;

public class EmployeImpl {

	public static void main(String[] args) {
		Employee employee01 = new Employee(1);
		
		employee01.setFirstName("Victor");
		employee01.setSalary(1000);
		
		//Los tipos de datos primitivos no contienen atributos ni m√©todos
		int id = employee01.getId();
		
		String nombre = employee01.getFirstName();
		String variable = new String();
		nombre = nombre.toUpperCase();
		//Diferencia entre tipos de datos primitivos y no primitivos
		// Los tipos de datos no primitivos son clases (contienen atributos)
		// Los tipos de datos primitivos no contienen atributos
		
		
		System.out.println("El id del empleado 01 es " + id);
		System.out.println("El empleado " + id + 
				" tiene el nombre de " + nombre);

	}

}
